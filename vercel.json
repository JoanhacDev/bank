{
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.)",
            "dest": "/"
        },
        {
            "src": "/styles/(.*)",
            "dest": "/public/styles/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        }
    ]
}